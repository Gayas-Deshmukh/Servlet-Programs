<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ServletConfig_(4)</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>test.ServletConfigDemo1</servlet-class>
    <init-param>
      <param-name>language</param-name>
      <param-value>JAVA</param-value>
    </init-param>
    <init-param>
      <param-name>version</param-name>
      <param-value>1.8</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/firstlink</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>servlet2</servlet-name>
    <servlet-class>test.ServletConfigDemo2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet2</servlet-name>
    <url-pattern>/secondlink</url-pattern>
  </servlet-mapping>
</web-app>